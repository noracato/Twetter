<p id="notice"><%= notice %></p>

<h1>My subscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Feeder id's</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscriptions.each do |subscription| %>
      <tr>
        <td><%= subscription.feeder_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub', new_subscription_path %>
<%= link_to 'My Feed', feed_index_path %>
